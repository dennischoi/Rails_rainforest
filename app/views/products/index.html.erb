<h1>All Products</h1>
<p>
  <%= link_to "Log IN", new_session_path %>, <%= link_to "Sign Up", new_user_path %>
</p>
  <%= link_to "New Product", new_product_path %>
    <% @products.each do |product| %>
    <p>
      <%= link_to product.name, product_path(product.id) %>, <%= product.formatted_price %>
    </p>
    <p>
      <%= product.description %>
    </p>


<% end %>
